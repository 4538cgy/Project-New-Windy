<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".bottomsheet.malloption.BottomSheetDialogMallAdmin">

    <data>
        <variable
            name="fragmentbottomsheetdialogmalladmin"
            type="com.uos.project_new_windy.bottomsheet.malloption.BottomSheetDialogMallAdmin" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <LinearLayout
            android:id="@+id/fragment_bottom_sheet_dialog_mall_admin_linear_top_bar"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/fragment_bottom_sheet_dialog_mall_admin_textview_orderinfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorBlack"
                android:layout_margin="12dp"
                android:text="주문 정보"
                android:textSize="18dp"
                />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>