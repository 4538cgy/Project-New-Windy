<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".bottomsheet.malloption.BottomSheetDialogMallOption">

    <data>
        <variable
            name="fragmentbottomsheetdialogmall"
            type="com.uos.project_new_windy.bottomsheet.malloption.BottomSheetDialogMallOption" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragment_bottom_sheet_dialog_mall_option_const_main_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                >
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="인기매물"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="농산물"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="축산물"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="소모품"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="농지"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="가구"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />
                <TextView
                    android:id="@+id/fragment_bottom_sheet_dialog_mall_option_textview_etc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="기타"
                    android:textStyle="bold"
                    android:textSize="18dp"
                    android:textColor="@color/colorBlack"
                    android:layout_margin="12dp"
                    android:onClick="@{fragmentbottomsheetdialogmall.groupClick}"
                    />





            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>